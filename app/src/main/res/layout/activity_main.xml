<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mca.hack.sponsorhub.MainActivity">

    <Button
        android:id="@+id/button_organiser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Organiser"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="97dp"
        android:layout_marginEnd="131dp"
        android:layout_marginTop="53dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/button_sponsor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sponsor"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="209dp"
        android:layout_below="@+id/button_organiser"
        android:layout_toEndOf="@+id/button_organiser"
        android:layout_marginStart="58dp"
        android:layout_marginTop="104dp" />

</android.widget.RelativeLayout>
